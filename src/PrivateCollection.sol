    //     ) {
    //         require(
    //             hasRole(TRANSFER_ROLE, from) || hasRole(TRANSFER_ROLE, to),
    //             "restricted to TRANSFER_ROLE holders"
    //         );
    //     }
    // }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(
            ERC721Enumerable,
            IERC165
        )
        returns (bool)
    {
        return
            super.supportsInterface(interfaceId) ||
            interfaceId == type(IERC2981).interfaceId;
    }

    function getCharityInfoForToken(uint256 tokenId)
        public
        view
        returns (address, uint16)
    {
        CharityInfo memory charityForToken = charityInfoForToken[tokenId];
        return (charityForToken.recipient, uint16(charityForToken.bps));
    }

    function stringToBytes32(string memory source)
        public
        pure
        returns (bytes32 result)
    {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            result := mload(add(source, 32))
        }
    }

    function getChainID() public view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }
